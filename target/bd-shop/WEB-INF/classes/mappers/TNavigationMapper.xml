    <!--@mbg.generated-->
    <id column="nav_id" jdbcType="INTEGER" property="navId" />
    <result column="nav_name" jdbcType="VARCHAR" property="navName" />
    <result column="crate_time" jdbcType="TIMESTAMP" property="crateTime" />
